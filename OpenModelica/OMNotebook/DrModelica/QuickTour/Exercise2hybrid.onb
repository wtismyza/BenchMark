            </TextCell>
            <GraphCell style="Graph" closed="false">
              <Input>simulate( LightSwitch, stopTime=20 ) </Input>
              <Output>record SimulationResult
    resultFile = "LightSwitch_res.mat",
    messages = ""
end SimulationResult;
